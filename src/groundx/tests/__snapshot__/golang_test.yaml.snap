        metadata:
          annotations:
            config-hash: 3460d7c7013557bc989e1f0c0bee3712a514df40b88cf33380f43810efa8c7c4
          labels:
            app: groundx
        spec:
          containers:
            - image: public.ecr.aws/c9r4x6y5/eyelevel/groundx:0.1.0
              imagePullPolicy: IfNotPresent
              livenessProbe:
                httpGet:
                  path: /health
                  port: 8080
                initialDelaySeconds: 30
                periodSeconds: 30
              name: groundx
