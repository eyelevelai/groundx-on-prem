'aws: cache.yaml':
'aws: cache-metrics.yaml':
'default: cache-metrics.yaml':
  1: |
    apiVersion: apps/v1
    kind: StatefulSet
    metadata:
      labels:
        app: cache-metrics
        appVersion: 0.1.0
        chart: groundx-0.1.0
        heritage: Helm
        version: 0.1.0
      name: cache-metrics
      namespace: eyelevel
    spec:
      replicas: 1
      selector:
        matchLabels:
          app: cache-metrics
      serviceName: cache-metrics
      template:
        metadata:
          labels:
            app: cache-metrics
        spec:
          containers:
            - image: public.ecr.aws/c9r4x6y5/eyelevel/redis:latest
              imagePullPolicy: Always
              name: cache-metrics
              ports:
                - containerPort: 6379
                  protocol: TCP
              resources:
                requests:
                  cpu: 200m
                  memory: 512Mi
              securityContext:
                allowPrivilegeEscalation: false
                capabilities:
                  drop:
                    - ALL
                runAsGroup: 1001
                runAsNonRoot: true
                runAsUser: 1001
                seccompProfile:
                  type: RuntimeDefault
              volumeMounts:
                - mountPath: /mnt/redis
                  name: cache-metrics-persistent-storage
          nodeSelector:
            node: eyelevel-cpu-only
          securityContext:
            fsGroup: 1001
            fsGroupChangePolicy: OnRootMismatch
            runAsGroup: 1001
            runAsNonRoot: true
            runAsUser: 1001
            seccompProfile:
              type: RuntimeDefault
          tolerations:
            - effect: NoSchedule
              key: node
              value: eyelevel-cpu-only
          volumes:
            - emptyDir: {}
              name: cache-metrics-persistent-storage
  2: |
    apiVersion: v1
    kind: Service
    metadata:
      labels:
        app: cache-metrics
      name: cache-metrics
      namespace: eyelevel
    spec:
      ports:
        - port: 6379
          protocol: TCP
          targetPort: 6379
      selector:
        app: cache-metrics
      type: ClusterIP
