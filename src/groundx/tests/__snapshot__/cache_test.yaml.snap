        heritage: Helm
        version: 0.1.0
      name: myapp-myapp
      namespace: eyelevel
    spec:
      replicas: 1
      selector:
        matchLabels:
          app: myapp-myapp
      serviceName: myapp-myapp
      template:
        metadata:
          annotations:
            prometheus.io/port: "8081"
          labels:
            app: myapp-myapp
            app.kubernetes.io/name: myapp
